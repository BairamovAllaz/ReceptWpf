<UserControl x:Class="ReceptWpf.App.Components.NavPages.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ReceptWpf.App.Components.NavPages"
             xmlns:test="clr-namespace:ReceptWpf.App.Components.NavPages.Show"
             mc:Ignorable="d"
             d:DesignHeight="750" d:DesignWidth="1000">
    <UserControl.Resources>
        <ResourceDictionary Source="HomeStyle.xaml"/>
    </UserControl.Resources>
    <ScrollViewer VerticalScrollBarVisibility="Auto"
                  Width="1000"
                  Height="650"
                  HorizontalAlignment="Left"
                  VerticalAlignment="Top"
                  CanContentScroll="False">
        <WrapPanel  x:Name="Stack"
                    Style="{StaticResource StyleStack}">
        </WrapPanel>
    </ScrollViewer>
</UserControl>
