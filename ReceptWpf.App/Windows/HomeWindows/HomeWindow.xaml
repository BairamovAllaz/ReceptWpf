<Window x:Class="ReceptWpf.App.Windows.HomeWindows.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ReceptWpf.App.Windows.HomeWindows"
        mc:Ignorable="d"
        Title="HomeWindow" Height="450" Width="800"
        DataContext="{Binding RelativeSource={x:Static RelativeSource.Self}}">
    <StackPanel>
        <TextBlock Text="{Binding user.FirstName}"/>
        <TextBlock Text="{Binding user.LastName}"/>
        <TextBlock Text="{Binding user.Email}"/>
        <TextBlock Text="{Binding user.Phone_number}"/>
        <TextBlock Text="{Binding user.Password}"/>
    </StackPanel>
</Window>
